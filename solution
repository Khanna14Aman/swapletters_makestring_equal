#include<bits/stdc++.h>
#include<iostream>
#include<vector>
#include<string>
using namespace std;
int main()
{
    int n,i;
    cin>>n;
    string s,t;
    cin>>s>>t;
    vector<int> v1,v2;
    
    for(i=0;i<n;i++)
    {
        if(s[i]=='a' && t[i]=='b')           //storing indexes of string s and t where they have "a"
        {
            v1.push_back(i);
        }
        else if(s[i]=='b' && t[i]=='a')
        {
            v2.push_back(i);
        }
    }
    vector<pair<int,int>>ans;
    int j,k;
    if(v1.size()%2 != v2.size()%2)   //checking that "a" and "b" are even in no so that they can devide equal.
    {
        cout<<-1;
        return 0;
    }
    else
    {
        while(v1.size() && v1.size()>1)
        {
            j = v1.back();
            v1.pop_back();
            k=v1.back();
            v1.pop_back();
            swap(s[j],t[k]);
            ans.push_back({j,k});
        }
        while(v2.size() && v2.size()>1)
        {
            j= v2.back();
            v2.pop_back();
            k= v2.back();
            v2.pop_back();
            swap(s[j],t[k]);
            ans.push_back({j,k});
        }
        if(v1.size() && v2.size())
        {
            j= v1.back();
            v1.pop_back();
            k= v2.back();
            v2.pop_back();
            swap(s[j],t[j]);
            ans.push_back({j,j});
            swap(s[j],t[k]);
            ans.push_back({j,k});
        }
        int x= ans.size();
        cout<<x<<"\n";
        for(i=0;i<x;i++)
        {
            cout<<ans[i].first+1<<" "<<ans[i].second+1<<"\n";//printing indexes from 1 to n.
        }
        
    }
    return 0;
}
